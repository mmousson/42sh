echo $((1-2))
echo $((1+1))$((2+2))$((3+3))
a=1+1; echo $((${a}))
b=4; echo $((b++)) $((++b)) $((--b)) $((b--))
echo $((65536 * 256 / 1024 % 2147483648))
echo $((2<3>0<=1>=2==0!=1))
c=8; echo $((123<=123 && 42%42 || c++ + 34))
echo $((0 / 0))
echo $((1 / 0.0))
echo $((1++))
echo $((1+))
echo $((3*12*(16-4+6/3)+42-(1*(34-5))))
echo $((0.0 % 0))
echo $((1 % 0))
